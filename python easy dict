#python dict
phone=input("Input phone: ")
digit={"0":"zero",
"1":"one", 
"2":"two",
'3':"three",
"4":"four",
"5":"five",
"6":"six",
"7":"seven",
"8":"eight",
"9":"nine"}
output=" "
for ch in phone:
    output+=digit.get(ch," ")+" "
print(output)
#Emoji converter
message = input("> ")
words = message.split( " ")
emojis = {
   ":)" : "ğŸ˜€",
   ":(" : "ğŸ˜",
   "lol" : "ğŸ˜‚",
   "sick":"ğŸ˜¨",
   "happy": "ğŸ˜€",
   "mermaid": "ğŸ§œâ€"
}
outcome = " "
for word in words:
   outcome += emojis.get(word, word) + " "
print(outcome)

